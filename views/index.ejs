<%- include('header') %>

<main>
  <div class="grid-row">
    <% mydata.forEach((toy)=> { %>
    <div class="grid-item">
      <%= toy.title.slice(0, 30) %>
      <button class="mybutton" type="button">
        <a class="mybuttontext" href="<%= toy.url %>" target=”_blank”>BUY</a>
      </button>
    </div>
    <% }) %>
  </div>
</main>

<%- include('footer') %>
